<?xml version="1.0" encoding="UTF-8"?>
<themes>

	<theme name="customize-widget" ref="base-tabbed-frame-customization">
	<param name="titleAreaTop"><int>10</int></param>
	<param name="titleAreaLeft"><int>12</int></param>
	<param name="titleAreaRight"><int>-30</int></param>
	<param name="titleAreaBottom"><int>30</int></param>
	<param name="closeButtonX"><int>-28</int></param>
	<param name="closeButtonY"><int>12</int></param>
		<param name="border"><border>35,10,10,10</border></param>
		<param name="minWidth"><int>600</int></param>
		<param name="minHeight"><int>350</int></param>
		<theme name="dialoglayout" ref="dialoglayout">
			<theme name="dialoglayout" ref="dialoglayout">
				<theme name="combobox" ref="combobox" allowWildcard="true">
					<param name="minWidth"><int>200</int></param>
				</theme>
				<theme name="editfield" ref="editfield">
					<theme name="renderer" ref="renderer">
						<param name="textAlignment"><enum type="alignment">left</enum></param>
					</theme>
				</theme>
			</theme>
			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="namedGaps"><map/></param>
		</theme>
	</theme>

	<theme name="customization-label" ref="label" allowWildcard="true">
		<param name="font"><font>pb-dark</font></param>
		<param name="textAlignment"><enum type="alignment">center</enum></param>
		<param name="background"><image>ui-header.background</image></param>
		<param name="border"><border>3,3</border></param>
		<param name="minWidth"><int>144</int></param>
		<param name="minHeight"><int>41</int></param>
	</theme>
	
	<theme name="customization-button-colorable" ref="button" allowWildcard="true">
		<param name=""><image>ui-green-button.*</image></param>
	</theme>
	
	<theme name="customization-save" ref="button" allowWildcard="true"/>
	
	<theme name="customization-color" ref="button" allowWildcard="true">
		<param name="font"><font>pb-dark</font></param>
		<param name="textAlignment"><enum type="alignment">center</enum></param>
		<param name="background"><image>none</image></param>
	</theme>
	
	<theme name="color-button" ref="button" allowWildcard="true">
		<param name="font"><font>pb-dark</font></param>
		<param name="textAlignment"><enum type="alignment">center</enum></param>
		<param name="background"><image>color-picker</image></param>
		<param name="minWidth"><int>20</int></param>
		<param name="minHeight"><int>20</int></param>
	</theme>
	
	<theme name="color-dialog" ref="-defaults" allowWildcard="true">
		<param name="font"><font>pb-dark</font></param>
		<param name="background"><image>none</image></param>
		<param name="border"><border>50,0,0,0</border></param>
		<param name="defaultGap"><dimension>5,5</dimension></param>
		<param name="smallGap"><dimension>8,8</dimension></param>
		<param name="mediumGap"><dimension>10,10</dimension></param>
		<param name="largeGap"><dimension>15,15</dimension></param>
		<param name="namedGaps"><map/></param>
	</theme>
	
	<theme name="tooltip-preview-widget" ref="-defaults">
		<param name="minWidth"><int>350</int></param>
		<theme name="dialoglayout" ref="-defaults" allowWildcard="true">
			<param name="background"><image>none</image></param>
			<param name="border"><border>5</border></param>
			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
			
			<theme name="preview-color-dialog" ref="color-dialog" allowWildcard="true">
				<param name="border"><border>25</border></param>
				<param name="background"><image>none</image></param>
			</theme>
			<theme name="label" ref="-defaults">
				<param name="font"><font>tooltip-font</font></param>
				<param name="textAlignment"><enum type="alignment">TOPLEFT</enum></param>
			</theme>
			<theme name="label-normal" ref="-defaults">
				<param name="font"><font>pb-dark</font></param>
				<param name="textAlignment"><enum type="alignment">TOPLEFT</enum></param>
			</theme>
		</theme>
	</theme>
	
	<theme name="previewwidget" ref="-defaults">
		<theme name="dialoglayout" ref="-defaults" allowWildcard="true">
			<param name="background"><image>ui-popup.background</image></param>
			<param name="border"><border>20,0,50,30</border></param>
			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
			
			<theme name="preview-color-dialog" ref="color-dialog" allowWildcard="true">
				<param name="border"><border>20,30,0,0</border></param>
				<param name="background"><image>none</image></param>
			</theme>
		</theme>
	</theme>

	<theme name="wardrobe" ref="new-base-frame">
		<param name="border"><border>35,10,3,5</border></param>
		<param name="minWidth"><int>745</int></param>
		<param name="minHeight"><int>300</int></param>
		<theme name="dialoglayout" ref="dialoglayout">
			<param name="border"><border>5,5,5,5</border></param>
			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
			<theme name="button" ref="button" allowWildcard="true">
				<param name="minWidth"><int>77</int></param>
			</theme>
			<theme name="dialoglayout" ref="dialoglayout">
				<param name="minWidth"><int>310</int></param>
				<theme name="boxlayout" ref="-defaults" allowWildcard="true">
					<param name="background"><image>none</image></param>
					<param name="border"><border>5,5,5,5</border></param>
					<param name="spacing"><int>5</int></param>
					<param name="alignment"><enum type="alignment">CENTER</enum></param>
					<theme name="label" ref="-defaults">
						<param name="textAlignment"><enum type="alignment">TOPLEFT</enum></param>
						<param name="minWidth"><int>40</int></param>
					</theme>
					<theme name="boxlayout" ref="-defaults" allowWildcard="true">
						<param name="background"><image>none</image></param>
						<param name="border"><border>5</border></param>
						<param name="spacing"><int>15</int></param>
						<param name="alignment"><enum type="alignment">CENTER</enum></param>
					</theme>
				</theme>
			</theme>
			<theme name="shoplayout" ref="dialoglayout">
				<param name="border"><border>5,15,5,0</border></param>
				<param name="minWidth"><int>350</int></param>
			</theme>
			<theme name="color-dialog" ref="-defaults" allowWildcard="true">
				<param name="font"><font>pb-dark</font></param>
				<param name="background"><image>none</image></param>
				<param name="border"><border>5,5,0,0</border></param>
				<param name="defaultGap"><dimension>5,5</dimension></param>
				<param name="smallGap"><dimension>8,8</dimension></param>
				<param name="mediumGap"><dimension>10,10</dimension></param>
				<param name="largeGap"><dimension>15,15</dimension></param>
				<param name="namedGaps"><map/></param>
			</theme>
		</theme>
		<theme name="button" ref="button">
			<param name="background"><image>ui-table-row.background</image></param>
			<param name="border"><border>0</border></param>
		</theme>
	</theme>

	<theme name="wardrobe-picker" ref="-defaults">
		<param name="background"><image>tooltip-left.background</image></param>
		<param name="minWidth"><int>280</int></param>
		<param name="minHeight"><int>240</int></param>
		<param name="border"><border>5,10</border></param>
		<theme name="scrollpane" ref="scrollpane">
			<theme name="dialoglayout" ref="-dialog">
				<theme name="boxlayout" ref="-defaults" allowWildcard="true">
					<param name="background"><image>none</image></param>
					<param name="border"><border>2</border></param>
					<param name="spacing"><int>5</int></param>
					<param name="alignment"><enum type="alignment">CENTER</enum></param>

					<theme name="wardrobe-addon-item" ref="button">
						<param name="border"><border>5</border></param>
						<param name="minWidth"><int>48</int></param>
						<param name="minHeight"><int>64</int></param>
					</theme>

					<theme name="wardrobe-addon-item-body" ref="button">
						<param name="border"><border>5</border></param>
						<param name="minWidth"><int>48</int></param>
						<param name="minHeight"><int>80</int></param>
					</theme>

					<theme name="wardrobe-addon-item-back" ref="button">
						<param name="border"><border>5</border></param>
						<param name="minWidth"><int>82</int></param>
						<param name="minHeight"><int>80</int></param>
					</theme>
				</theme>
			</theme>
		</theme>
	</theme>

	<theme name="wardrobe-picker-empty" ref="wardrobe-picker">
		<param name="minWidth"><int>280</int></param>
		<param name="minHeight"><int>40</int></param>
		<theme name="dialoglayout" ref="-dialog">
			<theme name="label" ref="-defaults">
				<param name="font"><font>pb-dark</font></param>
				<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
			</theme>
		</theme>
	</theme>

	<theme name="wardrobe-purchase-table" ref="table" allowWildcard="true">
		<param name="columnWidths">
			<map>
				<param name="0"><int>font.EM * 14</int></param>
				<param name="1"><int>font.EM * 5</int></param>
				<param name="2"><int>font.EM * 2</int></param>
				<param name="3"><int>font.EM * 2</int></param>
			</map>
		</param>
		<theme name="button" ref="button">
			<param name="background"><image>ui-table-row.background</image></param>
			<param name="border"><border>0</border></param>
		</theme>
		<theme name="button-wheel" ref="button">
			<param name="background"><image>ui-table-row.background</image></param>
			<param name="overlay"><image>icon.colorwheel</image></param>
			<param name="border"><border>0</border></param>
		</theme>
		<theme name="box-layout-row" ref="-defaults" allowWildcard="true">
			<param name="background"><image>ui-table-row.background</image></param>
			<param name="border"><border>0,0</border></param>
			<param name="spacing"><int>30</int></param>
			<param name="alignment"><enum type="alignment">CENTER</enum></param>
			<theme name="label" ref="label">
				<param name="border"><border>0,0</border></param>
			</theme>
		</theme>
	</theme>

	<theme name="addon-item-picker" ref="button">
		<param name="background"><image>ui-item-picker.background</image></param>
	</theme>
</themes>
