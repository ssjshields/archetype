	<theme name="tp-menu" ref="base-tabbed-frame" allowWildcard="true">
		<param name="background">
			<image>none</image>
		</param>
		<param name="border">
			<border>20,18,6,6</border>
		</param>
		<param name="minHeight">
			<int>500</int>
		</param>
		<param name="minWidth">
			<int>670</int>
		</param>
		<theme name="tabbedpane" ref="base-tabbed-pane">
			<param name="tabPosition">
				<enum type="tabposition">LEFT</enum>
			</param>
			<param name="background">
				<image>none</image>
			</param>
			<param name="border">
				<border>25,0,0,0</border>
			</param>
			<theme name="tabbox" ref="tabbox">
				<param name="border">
					<border>0,10,0,20</border>
				</param>
				<param name="spacing">
					<int>5</int>
				</param>
				<param name="alignment">
					<enum type="alignment">FILL</enum>
				</param>
				<theme name="tabbutton" ref="togglebutton">
					<param name="border">
						<border>5,0,5,10</border>
					</param>
					<param name="">
						<image>ui-tab-vertical.*</image>
					</param>
					<param name="font">
						<font>pb-tab</font>
					</param>
					<param name="textAlignment">
						<enum type="alignment">LEFT</enum>
					</param>
				</theme>
			</theme>
			<theme name="container" ref="container">
				<theme name="scrollpane" ref="scrollpane">
					<theme name="label" ref="label">
						<param name="border">
							<border>5</border>
						</param>
					</theme>
					<theme name="button" ref="button">
						<param name="minWidth">
              <int>130</int>
						</param>
					</theme>
				</theme>
			</theme>
		</theme>
	</theme>

  <theme name="tp-direction-widget" ref="resizableframe" allowWildcard="true">
    <param name="minWidth">
      <int>120</int>
    </param>
    <param name="minHeight">
      <int>170</int>
    </param>
    <theme name="button" ref="-defaults" allowWildcard="true">
      <param name="font">
        <font>alphabeta</font>
      </param>
      <param name="border">
        <border>0</border>
      </param>
      <param name="textAlignment">
        <enum type="alignment">RIGHT</enum>
      </param>
    </theme>
  </theme>

	<theme name="bulk-commands" ref="adminframe" allowWildcard="true">
		<theme name="scrollpane" ref="scrollpane">
			<param name="minWidth">
				<int>450</int>
			</param>
			<param name="minHeight">
				<int>500</int>
			</param>
			<theme name="editfield" ref="editfield" allowWildcard="true">
				<param name="minWidth">
					<int>450</int>
				</param>
				<param name="minHeight">
					<int>500</int>
				</param>
			</theme>
		</theme>
	</theme>
	
	<theme name="console" ref="-dialog" allowWildcard="true">
		<param name="background"><image>console.background</image></param>
		<theme name="dialoglayout" ref="-dialog" allowWildcard="true">
			<theme name="scrollpane" ref="scrollpane">
				<param name="background"><image>none</image></param>
				<param name="overlay"><image>none</image></param>
				<theme name="hscrollbar" ref="hscrollbar"/>  <!-- not used -->
				<theme name="vscrollbar" ref="vscrollbar-white"/>
				<param name="border"><border>0,0,0,0</border></param>
				
				<param name="maxHeight"><int>32767</int></param>
				<param name="hasDragButton"><bool>false</bool></param>
				<param name="autoScrollArea"><int>5</int></param>
				<param name="autoScrollSpeed"><int>10</int></param>
				<param name="scrollbarsAlwaysVisible"><bool>true</bool></param>
				<param name="minHeight"><int>40</int></param>
				<theme name="textarea" ref="-defaults">
					<param name="fonts"><map>
						<param name="default"><font>console</font></param>
						<param name="green"><font>console-green</font></param>
						<param name="red"><font>console-red</font></param>
						<param name="blue"><font>console-blue</font></param>
						<param name="orange"><font>console-orange</font></param>
						<param name="purple"><font>console-purple</font></param>
					</map></param>
					<param name="classes"><map/></param>
					<param name="images"><map>
						<param name="icon-gm"><image>icon-gm</image></param>
						<param name="icon-cm"><image>icon-cm</image></param>
						<param name="icon-hgm"><image>icon-hgm</image></param>
						<param name="icon-dev"><image>icon-dev</image></param>
						<param name="icon-sgm"><image>icon-sgm</image></param>
						<param name="icon-jgm"><image>icon-jgm</image></param>
						<param name="icon-mod"><image>icon-mod</image></param>
						<param name="icon-adm"><image>icon-admin</image></param>
						<param name="icon-friend"><image>icon7</image></param>
					</map></param>
				</theme>
			</theme>
			<theme name="editfield" ref="-defaults" allowWildcard="true">
				<param name="font"><font>pb-dark</font></param>
				<param name="background"><image>none</image></param>
				<param name="selection"><image>editfield.selection</image></param>
				<param name="border"><border>3</border></param>
				<param name="passwordChar"><int>35</int></param>
				<param name="maxWidth"><int>32767</int></param>
				<param name="columns"><int>118</int></param>
				<param name="cursor"><image>console.cursor</image></param>
				<param name="mouseCursor"><cursor>cursor.text</cursor></param>
				<theme name="renderer" ref="-input"/>
				<param name="autocompletion-height"><int>250</int></param>
				<param name="minWidth"><int>278</int></param>
			</theme>
		</theme>
	</theme>
</themes>